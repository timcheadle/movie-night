<h1>Events</h1>

<a href="<%= new_event_path %>">Create Event</a>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Date/Time</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <th><%= link_to event.location, event %></th>
        <th><%= event.occurs_at %></th>
      </tr>
    <% end %>
  </tbody>
</table>
